<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
	<Nullable>enable</Nullable>

	<PackageProjectUrl>https://github.com/n-yousefi/AutoCache</PackageProjectUrl>
    <Description>A .NET cache wrapper; prevent waiting on DB Fetches and remove multiple DB accesses on cache misses.</Description>
    <Copyright>Â©2022 Naser Yousefi</Copyright>
    <RepositoryUrl>https://github.com/Syzuna/AutoCache</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>Cache;.Net;Redis;IMemoryCache;</PackageTags>
    <VersionPrefix>1.0.0</VersionPrefix>
	<VersionSuffix>pre</VersionSuffix>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0-rc.2.22472.3" />
  </ItemGroup>

</Project>
